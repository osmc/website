{{!< default}}
{{#post}}

<div class="row clearfix main {{post_class}}">
    <article>

        <header class="post-header">
            <h1 class="post-title">{{title}}</h1>
        </header>

        <section class="post-content">
            {{content}}
        </section>

    </article>
</div>

{{/post}}
